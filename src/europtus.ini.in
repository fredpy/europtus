path=@CMAKE_SOURCE_DIR@/domains
dbg_europa=Debug.cfg
# could be just PlannerConfig.xml but full path ensure that we load the correct one
solver_cfg=@CMAKE_SOURCE_DIR@/domains/PlannerConfig.xml
# could be just survey.nddl but full path ensure that we load the correct one
nddl=@CMAKE_SOURCE_DIR@/domains/survey.nddl

# end at the next 6pm 
end_date=18:00:00Z
# Trick you can also say end_date=42:00:00Z to say "6pm the day after"
# but hours are limitted to 2 digits so this trick will work only that far

# max duration set to 12 hours
duration=12:00:00
